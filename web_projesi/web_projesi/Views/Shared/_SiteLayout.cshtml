                    <h4>Adres Bilgileri</h4>
                    <p>
                        @ViewBag.Iletisim.Adres<br>
                        <strong>Telefon:</strong> @ViewBag.Iletisim.Telefon<br>
                        <strong>Fax:</strong>  @ViewBag.Iletisim.Fax<br>
                        <strong>Whatsapp:</strong> @ViewBag.Iletisim.Whatsapp<br>
                        <strong>Facebook:</strong> @ViewBag.Iletisim.Facebook<br>
                        <strong>Instagram:</strong> @ViewBag.Iletisim.Instagram<br>
                        <strong>Twitter:</strong>  @ViewBag.Iletisim.Twitter<br>

                    </p>

                </div>


                <div class="col-lg-3 col-md-6 footer-info">
                    <h4>Blog Yazıları</h4>
                    <ul>
                        @foreach (var item in ViewBag.Blog)
                        {
                            <li><a href="/Home/Blog/">@item.Baslik</a></li>
                        }

                    </ul>
                </div>
                <div class="col-lg-3 col-md-6 footer-links">
                    <h4>Hizmetlerimiz</h4>

                    @foreach (var item in ViewBag.Hizmetler)
                    {
                        <img src="@item.ResimURL" width="75" height="65" />
                    }


                </div>







            </div>

        </div>
        <div class="col-4">
            <ul class="social-links">
